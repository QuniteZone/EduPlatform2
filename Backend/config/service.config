nacos:
  enable: true #是否启用nacos
  ip: 127.0.0.1 #nacos主机地址
  port: 8848 #nacos端口
  namespace: public
  group: DEFAULT_GROUP
  service_data_id: service.config
  prefix: "uniplore"
  current_service: smart-approach   # 服务名
  current_service_port: 5001  #
  heartbeat_interval: 10  #心跳间隔10s
services:
  smart-approach: "EduPlatform2" # 服务名
    ip: 127.0.0.1 #本地ip
    name: smart-approach.uniplore  #服务名
    port: 5001